class Player :
    def __init__(self,joueur,score,scorenb):
        self.__player = joueur
        self.__scoresheet = score
        self.__scoreamount = scorenb

#scorenb = 1 par joueur 
   
class Karaoke : 
    def __init__(self,chanson0,chanson1,chanson2,chanson3,chanson4):
        self.__song = chanson0
        self.__song1 = chanson1
        self.__song2 = chanson2
        self.__song3 = chanson3
        self.__song4 = chanson4

    def getChanson(self):
        return self.__song

    def getChanson1(self):
        return self.__song1

    def getChanson2(self):
        return self.__song2

    def getChanson3(self):
        return self.__song3

    def getChanson4(self):
        return self.__song4

